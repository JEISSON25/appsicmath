<?xml version='1.0' encoding='utf-8'?>
<widget id="com.appsicmath" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SicMath</name>
    <description>
       Aplication SicMath
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Senelis y Yuliana
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-inappbrowser"/>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="logo_final.png" density="ldpi" />
        <icon src="logo_final.png" density="mdpi" />
        <icon src="logo_final.png" density="hdpi" />
        <icon src="logo_final.png" density="xhdpi" />
    </platform>
   
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-targetSdkVersion" value="28" />
</widget>
